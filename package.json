{
  "name": "react-scroll-lock-component",
  "version": "0.2.0",
  "description": "A React component wrapper that restricts scrolling capabilities of everything except for its children.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/*.@(js|jsx)'",
    "clean": "rm -rf ./lib",
    "build-modules": "babel ./src --optional runtime --out-dir ./lib --source-maps",
    "build": "npm run clean && npm run build-modules",
    "build-demo": "cd demo && webpack"
  },
  "keywords": [
    "React",
    "scroll",
    "lock",
    "scrolllock",
    "javascript",
    "component"
  ],
  "author": "Teddy Ortega <tortega128@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orteth01/react-scroll-lock-component"
  }
}
